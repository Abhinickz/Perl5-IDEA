<templateSet group="Template Toolkit 2">
    <template name="GET" value="GET $VAR$" description="GET directive" toReformat="false" toShortenFQNames="true">
        <variable name="VAR" expression="&quot;element&quot;" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Template Toolkit 2.command" value="true"/>
        </context>
    </template>
    <template name="SET" value="SET $KEY$ = $VALUE$" description="SET directive" toReformat="false"
              toShortenFQNames="true">
        <variable name="KEY" expression="&quot;key&quot;" defaultValue="" alwaysStopAt="true"/>
        <variable name="VALUE" expression="&quot;value&quot;" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="Template Toolkit 2.command" value="true"/>
        </context>
    </template>
    <template name="CALL" value="CALL $VAR$" description="CALL directive" toReformat="false" toShortenFQNames="true">
